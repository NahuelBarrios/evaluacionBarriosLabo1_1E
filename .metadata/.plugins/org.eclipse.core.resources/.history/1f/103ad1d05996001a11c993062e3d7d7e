#ifndef CLIENTE_H_
#define CLIENTE_H_

typedef struct
{
	int idCliente;
	char nombreCliente[51];
	char apellidoCliente[51];
	char cuilCliente[14];
	int isEmptyCliente;

}eCliente;

int inicializarCliente(eCliente array[], int size);

int altaCliente(eCliente array[], int size, int* contadorID);

#endif /* CLIENTE_H_ */
