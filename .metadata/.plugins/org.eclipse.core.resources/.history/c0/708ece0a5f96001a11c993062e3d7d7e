#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include "utn.h"
#include "prestamo.h"

int inicializarPrestamo(ePrestamo array[], int size)
{
    int retorno=-1;
    if(array!= NULL && size>0)
    {
        for(;size>0;size--)
        {
            array[size-1].isEmptyPrestamo=1;
        }
        retorno=0;
    }
    return retorno;
}

