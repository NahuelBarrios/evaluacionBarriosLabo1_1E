#ifndef PRESTAMO_H_
#define PRESTAMO_H_

typedef struct
{
	int idPrestamo;
	int idCliente;
	float importePrestamo;
	int cuotasPrestamo;
	int isEmptyPrestamo;

}ePrestamo;

int inicializarPrestamo(ePrestamo array[], int size);


#endif /* PRESTAMO_H_ */
