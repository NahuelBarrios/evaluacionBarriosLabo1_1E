#ifndef CLIENTE_H_
#define CLIENTE_H_

typedef struct
{
	int idCliente;
	char nombre[51];
	char apellido[51];
	int cuil;
	int isEmptyCliente;

}eCliente;

int inicializarCliente(eCliente array[], int size);

#endif /* CLIENTE_H_ */
